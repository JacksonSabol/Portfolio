{
  "name": "Portfolio",
  "version": "1.0.0",
  "description": "Portfolio Website for Jackson Sabol",
  "main": "server.js",
  "engines": {
    "node": "10.12.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksonSabol/Portfolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JacksonSabol/Portfolio/issues"
  },
  "homepage": "https://github.com/JacksonSabol/Portfolio#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "moment": "^2.24.0",
    "nodemailer": "^6.0.0"
  }
}
